<extension xmlns="http://ns.adobe.com/air/extension/20.0">

    <id>com.mesmotronic.ane.aircast</id>
    <versionNumber>1.0.0</versionNumber>
  
    <platforms>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAirCast.a</nativeLibrary>
                <initializer>AirCastInitializer</initializer> 
                <finalizer>AirCastFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="Android-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>aircast-jar.jar</nativeLibrary> 
                <initializer>com.mesmotronic.ane.aircast.AirCastExtension</initializer> 
                <finalizer>com.mesmotronic.ane.aircast.AirCastExtension</finalizer> 
            </applicationDeployment> 
        </platform>
        
        <platform name="Android-x86"> 
            <applicationDeployment> 
                <nativeLibrary>aircast-jar.jar</nativeLibrary> 
                <initializer>com.mesmotronic.ane.aircast.AirCastExtension</initializer> 
                <finalizer>com.mesmotronic.ane.aircast.AirCastExtension</finalizer> 
            </applicationDeployment> 
        </platform>
        
        <platform name="default"> 
            <applicationDeployment/> 
        </platform>
    
    </platforms>
  
</extension>